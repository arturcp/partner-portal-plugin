{
  "manifest_version": 2,
  "name": "Study Group Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://partner-portal.k8s-eu-central-1-theta.external.sam-app.ro/*/create-account"
      ],
      "run_at": "document_end",
      "js": [
        "constants.js",
        "generator.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
